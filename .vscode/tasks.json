{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "test",
			"args": [
				"--release",
				"--",
				"--nocapture",
			],
			"problemMatcher": [
				"$rustc",
				"$rust-panic"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "rust: cargo test",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": true
			}
		}
	]
}